{
  "name": "opendeved-docs",
  "version": "1.0.0",
  "description": "Caribbean Education Research Database",
  "main": "",
  "scripts": {
    "postinstall": "node ./node_modules/frontend-dependencies/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:OpenDevEd/Zotero-kerko-docs.git"
  },
  "//": "Browserslist taken from static/src/vendor/bootstrap/.browserslistrc",
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "author": "Open Development & Education",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "autoprefixer": "^9.7.0",
    "clean-css": "^4.2.1",
    "clean-css-cli": "^4.3.0",
    "frontend-dependencies": "^1.1.7",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {},
  "frontendDependencies": {
    "target": "static/src/vendor/",
    "packages": {
      "@fortawesome/fontawesome-free": {
        "version": "5.11.2",
        "src": "/*",
        "namespaced": true
      },
      "bootstrap": {
        "version": "4.3.1",
        "src": "{dist/css/*,scss,js/dist/*}",
        "namespaced": true
      },
      "jquery": {
        "version": "3.4.1",
        "src": "dist/*",
        "namespaced": true
      },
      "popper.js": {
        "version": "1.16.0",
        "src": "dist/umd/*",
        "namespaced": true
      }
    }
  }
}
